{
    "compilerOptions": {
        "lib": ["es2015"],
        "allowSyntheticDefaultImports": false,
        "allowJs": false,
        "checkJs": false,
        "esModuleInterop": false,
        "importHelpers": true,
        "incremental": true,
        "strict": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noErrorTruncation": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "noResolve": true,
        "noStrictGenericChecks": true
    }
}
